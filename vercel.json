{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.primeeco.lk"
        }
      ],
      "destination": "https://primeeco.lk/:splat",
      "permanent": true
    }
  ],
 {
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}

